// キャラクリエイト用アニメーションデータ

namespace gfl.cc.fb;

file_extension "ccacnj";

table CCAnimationParts {
  name:string;                  // パーツ名
  tracnjFilePathList:[string];  // Tracnjファイルパスリスト
}

table CCAnimationConfig {
  partsList:[CCAnimationParts];
  animationSlotCount:int = 1;
}

root_type CCAnimationConfig;

