# tracpj の互換用に置換 'defaultValue' -> 'default'
def tracpj_replaceDefaultValue(csfilePath)
    buf = nil
    File.open(csfilePath,"r") do |f|
        buf = f.read();
    end
    buf.gsub!('JsonProperty("defaultValue")', 'JsonProperty("default")')
    File.open(csfilePath,"w") do |f|
        f.write(buf);
    end
end

